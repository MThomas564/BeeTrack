{
  "name": "bee-track",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "runLocal": "concurrently \"npm run dev-node\" \"npm run dev\"",
    "dev": "ng serve --proxy-config proxy.conf.json --open",
    "dev-node": "tsx watch src/server",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:server": "tsc -p tsconfig.server.json",
    "start:server": "nodemon --watch src/server --exec ts-node src/server/index.ts",
    "start:dev": "concurrently \"npm run start\" \"npm run start:server\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.11",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "^17.3.11",
    "@angular/compiler": "^17.3.11",
    "@angular/core": "^17.3.11",
    "@angular/forms": "^17.3.11",
    "@angular/platform-browser": "^17.3.11",
    "@angular/platform-browser-dynamic": "^17.3.11",
    "@angular/router": "^17.3.11",
    "@angular/service-worker": "^17.3.11",
    "@popperjs/core": "^2.11.6",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "pg": "^8.12.0",
    "primeflex": "^3.3.1",
    "primeicons": "^7.0.0",
    "primeng": "^17.18.0",
    "remult": "^0.26.12",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.8",
    "@angular/cli": "~17.3.8",
    "@angular/compiler-cli": "^17.3.11",
    "@angular/localize": "^17.3.11",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~4.3.0",
    "concurrently": "^8.2.2",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "tsx": "^3.12.3",
    "typescript": "~5.4.5"
  }
}